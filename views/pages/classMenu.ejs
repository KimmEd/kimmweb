<ul class="cards">
  <a href="/hub/class/add" class="float">
    <i class="fa fa-plus ">
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus-lg my-float" viewBox="0 0 16 16">
      <path fill-rule="evenodd" d="M8 2a.5.5 0 0 1 .5.5v5h5a.5.5 0 0 1 0 1h-5v5a.5.5 0 0 1-1 0v-5h-5a.5.5 0 0 1 0-1h5v-5A.5.5 0 0 1 8 2Z"/>
    </svg>
  </i>
  </a>

  <% if (classes) { %> <% classes.forEach(element => { %>
  <li>
    <a href="/hub/class/<%= element.id %>" class="card">
      <img
        src="<%- element.img %>"
        class="card__image"
        alt="<%- element.name %> Image"
      />
      <div class="card__overlay">
        <div class="card__header">
          <svg class="card__arc" xmlns="http://www.w3.org/2000/svg">
            <path />
          </svg>
          <img
            class="card__thumb"
            src="<%- element.ownerAvatar %>"
            alt="<%- element.owner %> Avatar"
          />
          <div class="card__header-text">
            <h3 class="card__title"><%- element.name %></h3>
            <span class="card__status"
              ><%- element.lastUpdated || 'Data not available' %></span
            >
          </div>
        </div>
        <p class="card__description"><%- element.description %></p>
      </div>
    </a>
  </li>
  <% }) %> <% } %>
</ul>
